import sric::*;

fun testHashMap() {
    var map = HashMap$<String, Int>{ .defValue = 0; };
    map.set("1", 1);
    map.set("2", 2);

    var i =  map.get("1");
    printf("%d\n", i);
}

fun testHashMap2() {
    var map = HashMap$<Int, String>{ .defValue = ""; };
    map.set(1, "1");
    map.set(2, "2");

    var i =  map.get(1);
    printf("%s\n", i.c_str());
}