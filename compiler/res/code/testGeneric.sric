
private abstract struct NT$<T> {
    abstract operator fun plus(that: * T): * T;
}

struct A$<T : NT$<T>> {
    var i: T ;
    fun foo() mut : raw* T {
       return &i;
    }

    fun foo2(): T {
        return *(i + &i);
    }
}

fun gf$<T>(v: mut T): T {
    return move v;
}

fun main()
{
    var a = A$<Int>{};
    a.i = 2;
    var b : raw* Int = a.foo();

    var i: Int = gf$<Int>(1);
}
